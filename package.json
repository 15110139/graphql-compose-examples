{
  "name": "graphql-compose-examples",
  "version": "0.0.1",
  "description": "Examples of graphql-compose",
  "engines": {
    "node": "8.6.0",
    "yarn": "1.3.2"
  },
  "files": [
    "lib"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodkz/graphql-compose-examples.git"
  },
  "keywords": [
    "graphql",
    "graphql-compose",
    "graphql-compose-mongoose",
    "graphql-compose-elasticsearch"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodkz/graphql-compose-examples/issues"
  },
  "homepage": "https://github.com/nodkz/graphql-compose-examples",
  "dependencies": {
    "aws-sdk": "^2.219.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cors": "^2.8.4",
    "elasticsearch": "^14.2.2",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "0.13.2",
    "graphql-compose": "^3.1.1",
    "graphql-compose-aws": "^1.0.2",
    "graphql-compose-connection": "^3.0.0",
    "graphql-compose-elasticsearch": "^1.8.11",
    "graphql-compose-mongoose": "^3.1.2",
    "graphql-compose-pagination": "^3.0.0",
    "graphql-compose-relay": "^1.1.10",
    "graphql-playground-middleware-express": "^1.6.0",
    "mongoose": "^5.0.12"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "0.69.0",
    "jest": "^22.4.3",
    "mongodb-memory-server": "^1.7.3",
    "nodemon": "^1.17.3",
    "prettier": "^1.11.1",
    "request": "^2.85.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/examples"
    ]
  },
  "scripts": {
    "build": "npm run buildSchema",
    "seed": "./node_modules/.bin/babel-node ./scripts/seed.js",
    "buildSchema": "./node_modules/.bin/babel-node ./scripts/buildSchema.js",
    "start": "npm run seed && babel-node index.js",
    "start:watch": "nodemon index.js -e js --exec babel-node",
    "watch": "jest --watch",
    "coverage": "jest --coverage --maxWorkers 2",
    "lint": "eslint --ext .js ./examples",
    "flow": "./node_modules/.bin/flow",
    "test": "npm run coverage && npm run lint && npm run flow"
  }
}
